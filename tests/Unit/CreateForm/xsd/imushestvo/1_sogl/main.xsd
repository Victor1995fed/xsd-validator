            </xsd:element>
            <xsd:element name="PhoneNumber" type="tns:string-16" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Телефон</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Email" type="tns:EmailAddressType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>E-mail</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DocumentDataType">
        <xsd:annotation>
            <xsd:documentation>Документ, удостоверяющий личность</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="DocumentName" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Наименование документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DocumentSeries" type="tns:string-10">
                <xsd:annotation>
                    <xsd:documentation>Серия документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DocumentNumber" type="tns:string-10">
                <xsd:annotation>
                    <xsd:documentation>Номер документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IssueDate" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation>Дата выдачи</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Issuer" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Кем выдан</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AuthorityDocumentDataType">
        <xsd:annotation>
            <xsd:documentation>Сведения о документе, подтверждающем полномочия законного представителя</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="DocumentName" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Наименование документа подтверждающего полномочия</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DocumentSeries" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Серия документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DocumentNumber" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Номер документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IssueDate" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation>Дата выдачи</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Issuer" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Кем выдан документ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PersonApplicantDataType">
        <xsd:annotation>
            <xsd:documentation>Сведения о физическом лице</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="LastName" type="tns:string-60">
                <xsd:annotation>
                    <xsd:documentation>Фамилия</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FirstName" type="tns:string-60">
                <xsd:annotation>
                    <xsd:documentation>Имя</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MiddleName" type="tns:string-60" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Отчество</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DocumentData" type="tns:DocumentDataType">
                <xsd:annotation>
                    <xsd:documentation>Документ, удостоверяющий личность</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ContactData" type="tns:PersonContactDataType">
                <xsd:annotation>
                    <xsd:documentation>Контактные данные</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RepresentativeDataType">
        <xsd:annotation>
            <xsd:documentation>Сведения о физическом лице</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="LastName" type="tns:string-60">
                <xsd:annotation>
                    <xsd:documentation>Фамилия</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FirstName" type="tns:string-60">
                <xsd:annotation>
                    <xsd:documentation>Имя</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MiddleName" type="tns:string-60" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Отчество</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DocumentData" type="tns:DocumentDataType">
                <xsd:annotation>
                    <xsd:documentation>Документ, удостоверяющий личность представителя</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ContactData" type="tns:RepresentativeContactDataType">
                <xsd:annotation>
                    <xsd:documentation>Контактные данные представителя</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ApplicantType">
        <xsd:annotation>
            <xsd:documentation>Сведения о заявителе</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="TargetCode" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Код цели</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="StatementDate" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation>Дата заполнения заявления</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="orderId" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation>Номер заявления</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Department" type="tns:DataElementType">
                <xsd:annotation>
                    <xsd:documentation>Код ведомства</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ResultMethod" type="tns:stringNN-50">
                <xsd:annotation>
                    <xsd:documentation>Способ получения результата</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MFCOffice" type="tns:DataElementType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Выбор отделения МФЦ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MFCagree" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Даю согласие на обработку персональных данных в МФЦ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AuthorityDocument" type="tns:AuthorityDocumentDataType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Сведения о документе, подтверждающем полномочия законного представителя</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="PersonApplicantData" type="tns:PersonApplicantDataType">
                    <xsd:annotation>
                        <xsd:documentation>Сведения о физическом лице</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="LegalApplicantData" type="tns:LegalApplicantDataType">
                    <xsd:annotation>
                        <xsd:documentation>Сведения о юридическом лице</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:choice>
                <xsd:element name="Representative" type="tns:RepresentativeDataType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Сведения о представителе - физическом лице</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="RepresentativeLegal" type="tns:RepresentativeLegalDataType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Сведения о представителе - юридическом лице</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="RequestData" type="tns:RequestDataType">
                <xsd:annotation>
                    <xsd:documentation>Данные заявления</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AppliedDocuments" type="tns:AppliedDocumentsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Приложенные документы</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AppliedDocumentsType">
        <xsd:annotation>
            <xsd:documentation>Группа вложений</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AppliedDocument" type="tns:AppliedDocumentType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Список вложений</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AppliedDocumentType">
        <xsd:sequence>
            <xsd:element name="CodeDocument" type="tns:stringNN-50" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Код документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Name" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Имя документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Number" type="tns:string-255" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Номер документа (по порядку)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="URL" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Относительный путь к файлу внутри архива</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Type" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>MIME-тип контента</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BusinessName" type="tns:string-255" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Бизнес-наименование документа</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="ID" type="tns:string-50" use="optional">
            <xsd:annotation>
                <xsd:documentation>Уникальный идентификатор вложения</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:element name="Applicant" type="tns:ApplicantType">
        <xsd:annotation>
            <xsd:documentation>Данные формы заявления</xsd:documentation>
        </xsd:annotation>
    </xsd:element>

</xsd:schema>
