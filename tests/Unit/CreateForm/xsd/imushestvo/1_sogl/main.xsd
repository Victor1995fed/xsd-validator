            <xsd:element name="Issuer" type="tns:string-255" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Орган, выдавший решение</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="WithdrawalDocumentType">
