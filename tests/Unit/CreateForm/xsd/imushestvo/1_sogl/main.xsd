                <xsd:annotation>
                    <xsd:documentation>Орган, выдавший решение</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RequestDataType">
        <xsd:annotation>
            <xsd:documentation>Ходатайство о переводе</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ObjectType" type="tns:DataElementType">
                <xsd:annotation>
                    <xsd:documentation>Тип объекта</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CadNo" type="tns:string-255" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Кадастровый номер земельного участка</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PartCadNo" type="tns:string-255" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Кадастровый номер части земельного участка</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LandSurveyingProjectDocument" type="tns:LandSurveyingProjectDocumentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Реквизиты решения об утверждении проекта межевания территории</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SourceCadNo" type="tns:string-255" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Кадастровый номер исходного земельного участка или земельных участков, из которых в соответствии с проектом межевания территории, со схемой расположения земельного участка предусмотрено образование испрашиваемого земельного участка</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Law" type="tns:DataElementType">
                <xsd:annotation>
                    <xsd:documentation>Вид права</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="PaymentBasis" type="tns:DataElementType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Основание предоставления земельного участка без проведения торгов (в собственность за плату)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="FreeBasis" type="tns:DataElementType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Основание предоставления земельного участка без проведения торгов (в собственность бесплатно)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="RentBasis" type="tns:DataElementType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Основание предоставления земельного участка без проведения торгов (в аренду)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="PerpetualBasis" type="tns:DataElementType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Основание предоставления земельного участка без проведения торгов (в постоянное (бессрочное) пользование)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="UnpaidBasis" type="tns:DataElementType" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Основание предоставления земельного участка без проведения торгов (в безвозмездное пользование)</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="Target" type="tns:string-255">
                <xsd:annotation>
                    <xsd:documentation>Цель использования земельного участка</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="WithdrawalDocument" type="tns:WithdrawalDocumentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Реквизиты решения об изъятии земельного участка для государственных или муниципальных нужд</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TerritorialPlanningDocument" type="tns:TerritorialPlanningDocumentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Реквизиты решения об утверждении документа территориального планирования и (или) проекта планировки территории (в  случае, если земельный участок предоставляется для размещения объектов, предусмотренных указанными документом и (или) проектом)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

